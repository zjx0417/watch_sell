oPlay()}),t.on("click",function(){e.removeClass("h")&&t.addClass("h")&&o.addClass("h"),window.player.videoPause()})},e.goodsList=function(){$(".view-goodsList").find("a").on("click",function(){var e=$(this).attr("data-code"),t=$(this).attr("data-name"),o=$(this).attr("data-brandCode"),a=$(this).attr("data-brandn"),i=$(this).attr("data-shopCode"),n=$(this).attr("data-shopName"),r=$(this).attr("data-wbiao"),s=$(this).attr("data-catalog"),c=new wb.Logger,l=$(".wbLogger"),d={event:"viewRcmdGoods",eventPage:"goodsDetailPage",eventPageType:"pc",eventProps:{viewCode:e,viewName:t,rcmdBrandCode:o,rcmdBrandName:a,rcmdShopCode:i,rcmdShopName:n,rcmdWbiaoPrice:r,rcmdCategoryCode:s,relatedGoodsCode:l.attr("viewCode"),relatedGoodsName:l.attr("viewName")}};c.log0.send(d),window.location.href="/goods/"+e+".html"})},$(".hot-auction .auction-item").click(function(){var e=$(".choice_content-a_title_bian").attr("data-index"),t=$(this).attr("data-auctionCode"),o=$(this).attr("data-brandName"),a=$(this).attr("data-brandCode"),i=$(this).attr("data-priceMin"),n=$(this).attr("data-priceMax"),r=$(this).attr("data-shopCode"),s=$(this).attr("data-shopName"),c=$(this).attr("data-brandUrl"),l=new wb.Logger,d={event:"clickHotAuction",eventPage:"goodsDetailPage",eventPageType:"html",eventProps:{goodsCode:e,auctionCode:t,brandName:o,brandCode:a,priceMin:i,priceMax:n,shopCode:r,shopName:s}};l.log0.send(d),window.open("/"+c+"/auction/"+t+".html")}),e.learnMore=function(){$(".js_more").on("click",function(){var e=new wb.Logger,t=$(".wbLogger");e.log0.send({event:"consultGoods",eventPage:"goodsDetailPage",eventPageType:"html",eventProps:{viewCode:t.attr("viewCode"),viewName:t.attr("viewName"),brandCode:t.attr("brandCode"),brandName:t.attr("brandName"),shopCode:t.attr("shopCode"),shopName:t.attr("shopName"),wbiaoPrice:t.attr("wbiaoPrice"),categoryCode:t.attr("categoryCode")}})})},e.isSeckilling=!1,e.checkSeckillActivity=function(t){var o=$(".js_share").attr("data-index");$.ajax({type:"post",dataType:"jso